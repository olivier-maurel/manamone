{% extends 'base.html.twig' %}

{% block title %}RowVirtual{% endblock %}

{% block body %}
    <h1>RowVirtual</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ row_virtual.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ row_virtual.name }}</td>
            </tr>
            <tr>
                <th>Value</th>
                <td>{{ row_virtual.value }}</td>
            </tr>
            <tr>
                <th>Debit</th>
                <td>{{ row_virtual.debit ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Modified_at</th>
                <td>{{ row_virtual.modifiedAt ? row_virtual.modifiedAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ row_virtual.createdAt ? row_virtual.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_row_virtual_index') }}">back to list</a>

    <a href="{{ path('app_row_virtual_edit', {'id': row_virtual.id}) }}">edit</a>

    {{ include('app/row_virtual/_delete_form.html.twig') }}
{% endblock %}
